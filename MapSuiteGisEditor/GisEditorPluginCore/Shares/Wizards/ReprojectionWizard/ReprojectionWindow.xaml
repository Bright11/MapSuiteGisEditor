<Window x:Class="ThinkGeo.MapSuite.GisEditor.Plugins.ReprojectionWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:ThinkGeo.MapSuite.GisEditor.Plugins"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:wizrd="clr-namespace:ThinkGeo.MapSuite.GisEditor.Plugins"
        Width="800"
        Height="600"
        ResizeMode="NoResize"
        Title="{DynamicResource ResourceKey=ReprojectionWindowTitle}"
        Style="{StaticResource WindowStyle}"
        mc:Ignorable="d">
    <Window.DataContext>
        <local:ReprojectionWizardViewModel x:Name="reprojectionWizardViewModel" />
    </Window.DataContext>
    <Grid>
        <wizrd:WizardView />
    </Grid>
</Window>