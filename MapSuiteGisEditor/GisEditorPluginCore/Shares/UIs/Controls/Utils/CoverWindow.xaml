<Window x:Class="ThinkGeo.MapSuite.GisEditor.Plugins.CoverWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="{DynamicResource ResourceKey=CoverWindowTitle}"
        Width="300"
        Height="300"
        AllowsTransparency="True"
        Cursor="/GisEditorPluginCore;component/Images/Eyedropper Cursor.cur"
        ShowInTaskbar="False"
        Focusable="True"
        Style="{StaticResource WindowStyle}"
        WindowStyle="None">
    <Window.Background>
        <SolidColorBrush Opacity="0.01" Color="White" />
    </Window.Background>
    <Grid />
</Window>