<WpfCustomFileDialog:WindowAddOnBase x:Class="ThinkGeo.MapSuite.GisEditor.Plugins.SaveFileDialogAddOn"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:WpfCustomFileDialog="clr-namespace:ThinkGeo.MapSuite.GisEditor;assembly=GisEditorToolkits"
             mc:Ignorable="d" 
             Loaded="TGProjPreviewControl_Loaded"
             Unloaded="TGProjPreviewControl_UnLoaded"
             Height="700" Width="350" Background="#fff0f0f0">
    <StackPanel>
        <TextBlock Text="{DynamicResource ResourceKey=OpenFileDialogAddOnProjectPreviewText}" Margin="0 8 0 0" />
        <Image Name="image" Grid.Row="1" Margin="0 12 20 0"  ></Image>
        <CheckBox Name="keepPassword"  Content="{DynamicResource KeepPasswordLable}" Foreground="Gray"  IsChecked="true" Margin ="0 8 0 0" IsEnabled="false" />
    </StackPanel>
</WpfCustomFileDialog:WindowAddOnBase>
